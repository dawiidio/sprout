    "cli",
    "command-line",
    "interface",
    "ai",
    "task",
    "issue"
  ],
  "author": {
    "email": "dawid@dawiid.io",
    "name": "Dawid Wojda",
    "url": "https://dawiid.io"
  },
  "bin": {
